SELECT
  submission_date_s3 AS submissionDate,
  funnelOrigin AS funnelOrigin
  sum(DAU) as DAU,
  sum(activeDAU) as aDAU,
  sum(installs) as installs,
  sum(searches) as searches
FROM
  `ga-mozilla-org-prod-001.telemetry.corpMetrics`
GROUP By 1,2
UNION ALL
SELECT
  submission_date_s3 AS submissionDate,
  'total' AS funnelOrigin
  sum(DAU) as DAU,
  sum(activeDAU) as aDAU,
  sum(installs) as installs,
  sum(searches) as searches
FROM
  `ga-mozilla-org-prod-001.telemetry.corpMetrics`
GROUP By 1,2
ORDER BY 1,2